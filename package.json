{
  "name": "qails",
  "version": "1.3.1",
  "description": "an ORM and RESTful nodejs web framework based Koa2 like Ruby on Rails. ",
  "repository": "qailsjs/qails",
  "main": "dist/index.js",
  "bin": {
    "babel": "./node_modules/.bin/babel",
    "babel-node": "./node_modules/.bin/babel-node",
    "eslint": "./node_modules/.bin/eslint",
    "knex": "./node_modules/.bin/knex"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "babel src -d dist",
    "start": "pm2 start ecosystem.config.js",
    "lint": "eslint --quiet \"**/*.js\"",
    "release": "standard-version",
    "prepublish": "npm run build",
    "postrelease": "git push && git push origin --tags",
    "test": "mocha --recursive --compilers js:babel-core/register ./test/index.js",
    "example:helloworld": "babel-node examples/helloworld/"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "bookshelf": "^0.10.3",
    "bookshelf-cascade-delete": "^2.0.1",
    "bookshelf-json-columns": "zhongzhi107/bookshelf-json-columns",
    "bookshelf-mask": "^2.0.1",
    "bookshelf-modelbase": "^2.10.3",
    "bookshelf-uuid": "^1.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-qunar": "^1.1.4",
    "eslint-plugin-import": "^2.2.0",
    "file-stream-rotator": "0.1.0",
    "joi": "^10.4.2",
    "knex": "^0.13.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compose": "^4.0.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-morgan": "^1.0.1",
    "koa-pug": "^3.0.0-2",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.1.1",
    "koa-session": "^5.0.0",
    "koa-static": "^3.0.0",
    "koa2-cors": "^2.0.3",
    "lodash": "^4.17.4",
    "minimatch": "^3.0.4",
    "mkdirp": "^0.5.1",
    "mysql": "^2.13.0",
    "object-assign": "^4.1.1",
    "packing-glob": "^1.0.1",
    "require-all": "^2.2.0",
    "statsd-client": "^0.2.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "mocha": "^3.4.2",
    "supertest": "^3.0.0"
  }
}
